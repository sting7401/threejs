{
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": true,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "endOfLine": "auto",
    "bracketSameLine": true,
    "vueIndentScriptAndStyle": true,
    "requirePragma": true,
    "insertPragma": false,
    "jsxSingleQuote": true,
    "enableDebugLogs": true
}
